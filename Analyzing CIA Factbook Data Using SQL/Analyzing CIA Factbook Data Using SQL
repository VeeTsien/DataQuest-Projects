    "%%sql\n",
    "SELECT name\n",
    "  FROM facts \n",
    " WHERE population > (SELECT AVG(population) \n",
    "                       FROM facts\n",
    "                      WHERE name NOT IN ('Antarctica', 'World') )\n",
    "   AND area < (SELECT AVG(area) \n",
    "                 FROM facts\n",
    "                WHERE name NOT IN ('Antarctica', 'World') );"
