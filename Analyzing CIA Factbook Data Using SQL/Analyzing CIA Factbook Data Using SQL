    "%%sql\n",
    "SELECT name, population, population_growth, CAST(population*population_growth AS INT) AS new_born\n",
    "  FROM facts\n",
    " WHERE name NOT IN ('Antarctica', 'World')\n",
    " ORDER BY population*(1+population_growth) DESC\n",
    " LIMIT 5"
   ]
